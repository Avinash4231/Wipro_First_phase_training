@page
@model RazorExample.Pages.EmployeeModel
@{
	<p>employee Data</p>

}
<table>
	<thead>
		<tr>
			<th>ID</th>
			<th>Name</th>
			<th>Gender</th>
			<th>Address</th>

		</tr>
	</thead>
	<tbody>
	@foreach(var emp in Model.employees){
		<tr>
			<td>@emp.Id</td>
			<td>@emp.Name</td>
			<td>@emp.Gender</td>
			<td>@emp.Address</td>



		</tr>
	}
	</tbody>
</table>
